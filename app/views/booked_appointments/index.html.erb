<nav class="navbar navbar-expand-sm bg-dark navbar-dark fixed-to justify-content-between">
  <ul class="navbar-nav">
    <li class="navbar-brand">
      <span class="nav-link"><%= link_to "book appointment", new_booked_appointment_path %></span>
    </li>
    <li class="navbar-brand">
      <span class="nav-link"> <%= link_to "Edit profile", edit_user_registration_path %></span>
    </li>
    <li class="navbar-brand">
     <span class="nav-link">  <%= link_to "Sign out", destroy_user_session_path %></span>
    </li>
    </ul>
    <ul class="navbar-nav">
    <li class="navbar-brande">
    <%= form_tag booked_appointments_path, class: "form-inline", :method => "get" do %>
        <%= text_field_tag :booking_id, "Search booking", class: "form-control mr-sm-2" %>
        <%= submit_tag "search", class: "btn btn-outline-success my-2 my-sm-0" %>
    <% end %>
    </li>
  </ul>
</nav>
<table class='table'>
  <thead>
    <tr>
      <th scope='col'>boking id</th>
      <th scope='col'>vehicle name</th>
      <th scope='col'>service</th>
      <th scope='col'>service date</th>
      <th scope='col'>pickup date</th>
      <th scope='col'>special request</th>
      <th scope='col'>employee name</th>
      <th scope='col'>drop time</th>
      <th scope='col'>pickup time</th>
      <th scope='col'>service status</th>
      <th scope='col'>service center</th>
    </tr>
  </thead>
  <tbody>
    <% @booked_appointments.each do |appoinment| %>
    <tr class="tr">
      <td scope='row'><%= appoinment.token %></td>
      <td scope='row'><%= appoinment.vehicle_name %></td>
      <td scope='row'><%= appoinment.service %></td>
      <td scope='row'><%= appoinment.service_date %></td>
      <td scope='row'><%= appoinment.pickup_date %></td>
      <td scope='row'><%= appoinment.special_request %></td>
      <td scope='row'><%= appoinment.employee_name %></td>
      <td scope='row'><%= appoinment.drop_time %></td>
      <td scope='row'><%= appoinment.pickup_time %></td>
      <td scope='row' id="service_status"><%= appoinment.service_status %></td>
      <td scope='row'><%= @servicecenter.name %></td>
      <td scope='row' id="edit_option"><%= link_to "edit", edit_booked_appointment_path(appoinment.id) %></td>
      <td scope='row' id="show_invoice" style="display:none"><%= link_to "show invoice", invoice_path(appoinment.id) %>
      </td>
      <td scope='row' id="show_option"><%= link_to "show", booked_appointment_path(appoinment.id) %></td>
      <td scope='row' id="delete_option"><%= link_to "delete", booked_appointment_path(appoinment.id), data: { method: :delete } %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>