<%=form_with model:@admin,url: superadmins_path,local: true do |f|%>
    
   <%f.select_field "companies",options_from_collection_for_select(companies.all,"id","name")%>

    <p>
        <%f.label :admin_name%>
        <%f.text_field :name%>
    </p>

    <p>
        <%f.label :password%>
        <%f.password_field :password%>
    </p>

    <p>
        <%f.label :password_confirmation%>
        <%f.password_field :password_confirmation%>
    </p>
    <%=f.submit "create admin"%>
<%end%>
