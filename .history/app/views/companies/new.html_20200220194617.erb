<%=form_with model:@company,url: companies_path,local: true do |c| %>
    <p>
        <%=c.label :company_name%>
        <%=c.text_field :name ,placeholder: "eg.Bajaj"%>
    <p>

    <p>
        <%=c.label :website%>
        <%=c.text_field :name ,placeholder: "eg.www.bajaj.com"%>
    <p>

    <p>
        <%=c.label :email%>
        <%=c.text_field :name ,placeholder: "eg.Bajaj@gmail.com"%>
    <p>
    <%=c.submit%>
<%end%>